        "message": "=<div style=\"font-family: Arial, sans-serif; padding: 20px;\">\n  <h2 style=\"color: #0ea5e9;\">New {{ $json.metadata?.submissionType || 'Contact' }} Lead</h2>\n  \n  <table style=\"width: 100%; border-collapse: collapse;\">\n    <tr>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\"><strong>Email:</strong></td>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\">{{ $json.email }}</td>\n    </tr>\n    <tr>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\"><strong>Name:</strong></td>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\">{{ $json.name || 'Not provided' }}</td>\n    </tr>\n    <tr>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\"><strong>Source:</strong></td>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\">{{ $json.source }}</td>\n    </tr>\n    <tr>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\"><strong>Timestamp:</strong></td>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\">{{ $json.timestamp }}</td>\n    </tr>\n    <tr>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\"><strong>Website:</strong></td>\n      <td style=\"padding: 10px; border-bottom: 1px solid #e2e8f0;\">{{ $json.website || 'Not provided' }}</td>\n    </tr>\n  </table>\n  \n  {{ $json.metadata?.submissionType === 'quiz' ? '<h3>Quiz Answers:</h3><ul><li><strong>Challenge:</strong> ' + ($json.metadata?.quizAnswers?.challenge || 'N/A') + '</li><li><strong>Current Solution:</strong> ' + ($json.metadata?.quizAnswers?.currentSolution || 'N/A') + '</li><li><strong>Business Stage:</strong> ' + ($json.metadata?.quizAnswers?.businessStage || 'N/A') + '</li><li><strong>Urgency:</strong> ' + ($json.metadata?.quizAnswers?.urgency || 'N/A') + '</li></ul><p><strong>Recommendation:</strong> ' + ($json.metadata?.recommendation || 'N/A') + '</p>' : '' }}\n  \n  {{ $json.message ? '<h3>Message:</h3><p>' +